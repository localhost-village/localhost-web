<style>
  .logo-container {
    width: 100%;
    max-width: 600px;
    margin: 0 auto;
    position: relative;
    text-align: center;
  }

  .main-logo {
    width: 100%;
    height: auto;
    opacity: 0;
    animation: logoAppear 1.5s cubic-bezier(0.4, 0, 0.2, 1) forwards;
  }

  .logo-light {
    display: none;
  }

  :global([data-theme="dark"]) .logo-dark {
      display: none;
    }

    :global([data-theme="dark"]) .logo-light {
      display: block;
    }

    .subline {
      margin-top: 1rem;
      color: #1a1f36;
      font-size: 1.3rem;
      font-weight: 400;
      opacity: 0;
      animation: textAppear 1.5s cubic-bezier(0.4, 0, 0.2, 1) forwards;
      animation-delay: 0.5s;
    }

    /* Fix for dark mode subline */
    :global([data-theme="dark"]) .subline {
      color: #ffffff !important;
      background-color: #1a1f36;
      border-radius: 3px;
    }

    @keyframes logoAppear {
      0% {
        opacity: 0;
        transform: scale(0.95) translateY(10px);
      }
      100% {
        opacity: 1;
        transform: scale(1) translateY(0);
      }
    }

    @keyframes textAppear {
      0% {
        opacity: 0;
        transform: translateY(10px);
      }
      100% {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .logo-container:hover .main-logo {
      filter: drop-shadow(0 4px 12px rgba(0, 0, 0, 0.1));
      transition: filter 0.3s ease;
    }

    :global([data-theme="dark"]) .logo-container:hover .main-logo {
      filter: drop-shadow(0 4px 12px rgba(255, 255, 255, 0.1));
    }
  </style>
